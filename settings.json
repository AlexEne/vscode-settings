{
    "editor.renderControlCharacters": true,
    "window.menuBarVisibility": "default",
    "debug.allowBreakpointsEverywhere": true,
    "window.zoomLevel": 0,
    "problems.decorations.enabled": true,
    "workbench.editor.showTabs": true,
    "telemetry.enableTelemetry": false,
    "zenMode.centerLayout": false,
    "workbench.iconTheme": "material-icon-theme",
    "window.titleBarStyle": "custom",
    "git.enableSmartCommit": true,
    "editor.trimAutoWhitespace": false,
    "editor.minimap.showSlider": "always",
    "terminal.integrated.windowsEnableConpty": true,
    "zenMode.hideLineNumbers": false,
    "editor.tokenColorCustomizations": {
        "[Material Theme Darker High Contrast]": {
            "textMateRules": [
                {
                    "scope": [
                        "punctuation.definition.comment",
                        "comment.block",
                        "comment.line",
                        "comment.block.documentation",
                        "comment"
                    ],
                    "settings": {
                        "foreground": "#9ca3af"
                    },
                },
                {
                    "scope": [
                        "Keyword",
                        "Storage",
                        "comment",
                        "keyword.control",
                        "entity.name.method.js",
                        "variable.other"
                    ],
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "variable.parameter"
                    ],
                    "settings": {
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": [
                        "keyword"
                    ],
                    "settings": {
                        "foreground": "#C2C2BD",
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": [
                        "variable.other"
                    ],
                    "settings": {
                        "foreground": "#cccbcb",
                    }
                }
            ]
        },
        "[Ayu Dark Bordered]": {
            "textMateRules": [
                {
                    "scope": [
                        "punctuation.definition.comment.begin",
                        "punctuation.definition.comment.end",
                        "comment.block.documentation",
                        "comment.block",
                        "comment.line",
                        "comment"
                    ],
                    "settings": {
                        "foreground": "#78862e"
                    },
                },
                {
                    "scope": [
                        "Keyword",
                        "Storage",
                        "keyword.control",
                        "variable.language",
                    ],
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "variable.other"
                    ],
                    "settings": {
                        "foreground": "#afafaa",
                    }
                },
                {
                    "scope": [
                        "variable.other.enummember"
                    ],
                    "settings": {
                        "foreground": "#ffb454",
                    }
                }
            ]
        }
    },
    "editor.semanticHighlighting.enabled": true,
    "editor.semanticTokenColorCustomizations": {
        "[Material Theme Darker High Contrast]": {
            "rules": {
                "operator.unsafe": "#f00",
                "function.unsafe": "#f00",
                "*.mutable": {
                    "fontStyle": ""
                },
                "parameter": {
                    "fontStyle": "bold",
                    "foreground": "#f0e4a7"
                }
            }
        },
        "[Ayu Dark Bordered]": {
            "rules": {
                "operator.unsafe": "#f00",
                "function.unsafe": "#f00"
            }
        }
    },
    "editor.autoClosingOvertype": "always",
    "terminal.integrated.rendererType": "experimentalWebgl",
    "files.autoSave": "onFocusChange",
    "glsllint.glslangValidatorPath": "G:\\Rust\\dwarf_game\\tools\\glslangValidator.exe",
    "editor.renderWhitespace": "none",
    "workbench.colorCustomizations": {
        "[Material Theme Darker High Contrast]": {},
        "[Ayu Dark Bordered]": {
            "foreground": "#9199aa",
            "tab.inactiveForeground": "#92959b",
            "tab.unfocusedInactiveForeground": "#8c919b",
            "sideBar.foreground": "#a3a3a3",
            "list.hoverForeground": "#cecbcb",
            "list.focusForeground": "#d8d6d6",
            "list.activeSelectionForeground": "#cecbcb",
            "list.inactiveSelectionForeground": "#cecbcb",
            "menu.foreground": "#a7a4a4",
            "panelTitle.inactiveForeground": "#b9b9b9",
            "editorIndentGuide.activeBackground": "#353535",
            "editorIndentGuide.background": "#1b1b1b",
            "editor.background": "#141414",
            "editor.lineHighlightBackground": "#0f0f0f"
        }
    },
    "workbench.colorTheme": "Ayu Dark Bordered",
    "git.autofetch": true,
    "python.showStartPage": false,
    "editor.minimap.maxColumn": 70,
    "cmake.configureOnOpen": false,
    "terminal.integrated.shell.windows": "powershell.exe",
    "rust-analyzer.cargo.loadOutDirsFromCheck": true,
    "rust-analyzer.inlayHints.typeHints": false,
    "rust-analyzer.inlayHints.parameterHints": false,
    "editor.minimap.enabled": true,
    // "workbench.editor.tabSizing": "shrink"
}
