    "zenMode.hideLineNumbers": false,
    "editor.smoothScrolling": true,
    "editor.tokenColorCustomizations": {
        "[Material Theme Darker High Contrast]": {
            "textMateRules": [
                {
                    "scope": [
                        "punctuation.definition.comment",
                        "comment.block",
                        "comment.line",
                        "comment.block.documentation",
                        "comment"
                    ],
                    "settings": {
                        "foreground": "#9ca3af"
                    }
                },
                {
                    "scope": [
                        "Keyword",
                        "Storage",
                        "comment",
                        "keyword.control",
                        "entity.name.method.js",
                        "variable.other"
                    ],
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "variable.parameter"
                    ],
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "keyword"
                    ],
                    "settings": {
                        "foreground": "#C2C2BD",
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "variable.other"
                    ],
                    "settings": {
                        // "foreground": "#e2e2e1",
                    }
                }
            ]
        },
        "[Ayu Dark Bordered]": {
            "textMateRules": [
                {
                    "scope": [
                        "punctuation.definition.comment.begin",
                        "punctuation.definition.comment.end",
                        "comment.block.documentation",
                        "comment.block",
                        "comment.line",
                        "comment"
                    ],
                    "settings": {
                        "foreground": "#78862e"
                    }
                },
                {
                    "scope": [
                        "Keyword",
                        "Storage",
                        "keyword.control",
                        "variable.language"
                    ],
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "variable.other"
                    ],
                    "settings": {
                        "foreground": "#afafaa"
                    }
                },
                {
                    "scope": [
                        "variable.other.enummember"
                    ],
                    "settings": {
                        "foreground": "#ffb454"
                    }
                }
            ]
        },
        "[Popping and Locking]": {
            "textMateRules": [
                {
                    "scope": [
                        "punctuation.definition.comment",
                        "comment.block",
                        "comment.line",
                        "comment.block.documentation",
                        "comment"
                    ],
                    "settings": {
                        "foreground": "#9ca3af"
                    }
                },
                {
                    "scope": [
                        "Keyword",
                        "Storage",
                        "comment",
                        "keyword.control",
                        "entity.name.method.js",
                        "variable.other"
                    ],
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "variable.parameter"
                    ],
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "variable.other"
                    ],
                    "settings": {
                        // "foreground": "#e2e2e1",
                    }
                }
            ]
        }
    },
    "editor.semanticHighlighting.enabled": true,
    "editor.semanticTokenColorCustomizations": {
        "[Material Theme Darker High Contrast]": {
            "rules": {
                "operator.unsafe": "#f00",
                "function.unsafe": "#f00",
                "*.mutable": {
                    "fontStyle": ""
                },
                "parameter": {
                    "fontStyle": "",
                    "foreground": "#C792EA"
                }
            }
        },
        "[Ayu Dark Bordered]": {
            "rules": {
                "operator.unsafe": "#f00",
                "function.unsafe": "#f00"
            }
        },
        "[Popping and Locking]": {
            "rules": {
                "operator.unsafe": "#f00",
                "function.unsafe": "#f00",
                "*.mutable": {
                    "fontStyle": ""
                },
                "parameter": {
                    "fontStyle": "",
                    "foreground": "#A989D2"
                }
            }
        }
    },
