{
    "editor.renderControlCharacters": true,
    "debug.allowBreakpointsEverywhere": true,
    "problems.decorations.enabled": true,
    "workbench.editor.showTabs": "multiple",
    "telemetry.telemetryLevel": "off",
    "zenMode.centerLayout": true,
    "workbench.iconTheme": "catppuccin-mocha",
    "window.titleBarStyle": "custom",
    "git.enableSmartCommit": true,
    "editor.trimAutoWhitespace": false,
    "editor.minimap.showSlider": "always",
    "terminal.integrated.windowsEnableConpty": true,
    "zenMode.hideLineNumbers": false,
    "editor.smoothScrolling": true,
    // "rust-analyzer.cargo.targetDir": "target-rust-analzyer",
    "editor.tokenColorCustomizations": {
        "[Catppuccin Mocha]": {
            "textMateRules": [
                {
                    "scope": [
                        "punctuation.definition.comment",
                        "comment.block",
                        "comment.line",
                        "comment.block.documentation",
                        "comment"
                    ],
                    "settings": {
                        "foreground": "#9ca3af"
                    }
                }
            ]
        },
        "[Popping and Locking]": {
            "textMateRules": [
                {
                    "scope": [
                        "punctuation.definition.comment",
                        "comment.block",
                        "comment.line",
                        "comment.block.documentation",
                        "comment"
                    ],
                    "settings": {
                        "foreground": "#9ca3af"
                    }
                },
                {
                    "scope": [
                        "Keyword",
                        "Storage",
                        "comment",
                        "keyword.control",
                        "entity.name.method.js",
                        "variable.other"
                    ],
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "variable.parameter"
                    ],
                    "settings": {
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "variable.other"
                    ],
                    "settings": {
                        // "foreground": "#e2e2e1",
                    }
                }
            ]
        }
    },
    "editor.semanticHighlighting.enabled": true,
    "editor.semanticTokenColorCustomizations": {
        "[Popping and Locking]": {
            "rules": {
                "operator.unsafe": "#f00",
                "function.unsafe": "#f00",
                "*.mutable": {
                    "fontStyle": ""
                },
                "parameter": {
                    "fontStyle": "",
                    "foreground": "#A989D2"
                }
            }
        },
        "[Catppuccin Mocha]": {
            "rules": {
                "operator.unsafe": "#ff2f2f",
                "function.unsafe": "#ff2f2f",
            }
        }
    },
    "editor.autoClosingOvertype": "always",
    "files.autoSave": "onFocusChange",
    "glsllint.glslangValidatorPath": "G:\\Rust\\dwarf_game\\tools\\glslangValidator.exe",
    "editor.renderWhitespace": "none",
    "workbench.colorCustomizations": {
        "[Popping and Locking]": {
            "editor.selectionBackground": "#414040",
            "editor.selectionHighlightBackground": "#414040",
            "editor.inactiveSelectionBackground": "#414040",
            "minimap.background": "#00000000",
            "minimap.foregroundOpacity": "#000000b0",
            "scrollbar.shadow": "#0000002a"
        },
        "[Catppuccin Mocha]": {
            "debugIcon.breakpointForeground": "#ff0000",
            "editor.stackFrameHighlightBackground": "#ff00003F",
            "editor.focusedStackFrameHighlightBackground": "#ff00003F"
        }
    },
    "git.autofetch": true,
    "editor.minimap.maxColumn": 70,
    "cmake.configureOnOpen": false,
    "editor.guides.bracketPairs": true,
    "materialTheme.accent": "Orange",
    "diffEditor.ignoreTrimWhitespace": false,
    "debug.disassemblyView.showSourceCode": false,
    // "workbench.editor.tabSizing": "shrink"
    "editor.cursorSurroundingLines": 6,
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "rust-analyzer.inlayHints.typeHints.enable": false,
    "remote.containers.dockerPath": "podman.exe",
    "docker.dockerPath": "podman",
    "remote.containers.cacheVolume": false,
    "editor.minimap.enabled": false,
    "terminal.integrated.shellIntegration.enabled": true,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "git.confirmSync": false,
    "editor.fontFamily": "'Monaspace Neon'",
    "editor.fontLigatures": "'calt', 'liga'",
    "editor.fontWeight": "normal",
    "editor.fontSize": 14,
    "arduino.useArduinoCli": true,
    "vim.easymotion": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "d"
            ],
            "after": [
                "\"",
                "_",
                "d"
            ]
        },
        {
            "before": [
                "D"
            ],
            "after": [
                "\"",
                "_",
                "D"
            ]
        },
        {
            "before": [
                "d",
                "d"
            ],
            "after": [
                "\"",
                "_",
                "d",
                "d"
            ]
        },
        {
            "before": [
                "<BS>"
            ],
            "after": [
                "\"",
                "_",
                "d"
            ]
        },
        {
            "before": [
                "<Tab>"
            ],
            "after": [
                "<S>",
                ">",
                "g",
                "v"
            ]
        },
        {
            "before": [
                "<S-Tab>",
            ],
            "after": [
                "<S>",
                "<",
                "g",
                "v"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "d"
            ],
            "after": [
                "\"",
                "_",
                "d"
            ]
        },
        {
            "before": [
                "D"
            ],
            "after": [
                "\"",
                "_",
                "D"
            ]
        },
        {
            "before": [
                "d",
                "d"
            ],
            "after": [
                "\"",
                "_",
                "d",
                "d"
            ]
        },
        {
            "before": [
                "<BS>"
            ],
            "after": [
                "\"",
                "_",
                "d"
            ]
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "d",
                "d"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                {
                    "command": "editor.action.showDefinitionPreviewHover"
                }
            ]
        },
        {
            "before": [
                "s"
            ],
            "after": [
                "leader",
                "leader",
                "s"
            ]
        },
        {
            "before": [
                "<Tab>"
            ],
            "after": [
                "<S>",
                ">",
                ">"
            ]
        },
        {
            "before": [
                "<S-Tab>",
            ],
            "after": [
                "<S>",
                "<",
                "<"
            ]
        }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false,
        "<C-p>": false,
        "<C-c>": false,
        "<C-s>": false,
        "<C-v>": false,
        "<C-w>": false,
        "<C-x>": false,
        "<C-d>": false,
        "<C-g>": false,
    },
    "extensions.experimental.affinity": {
        "vscodevim.vim": 1
    },
    "editor.cursorSmoothCaretAnimation": "on",
    "terminal.integrated.smoothScrolling": true,
    "terminal.integrated.scrollback": 10000,
    "debug.onTaskErrors": "abort",
    "security.workspace.trust.untrustedFiles": "open",
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "cmake.showOptionsMovedNotification": false,
    "lldb.suppressUpdateNotifications": true,
    "amazonQ.telemetry": false,
    "amazonQ.shareContentWithAWS": false,
    "amazonQ.workspaceIndex": true,
    "amazonQ.workspaceIndexUseGPU": true,
    "catppuccin.italicKeywords": false,
    "workbench.colorTheme": "Catppuccin Mocha",
    "catppuccin.italicComments": false,
    "catppuccin.boldKeywords": false
}
