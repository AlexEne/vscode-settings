    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": [
                "d"
            ],
            "after": [
                "\"",
                "_",
                "d"
            ]
        },
        {
            "before": [
                "D"
            ],
            "after": [
                "\"",
                "_",
                "D"
            ]
        },
        {
            "before": [
                "d",
                "d"
            ],
            "after": [
                "\"",
                "_",
                "d",
                "d"
            ]
        },
        {
            "before": [
                "<BS>"
            ],
            "after": [
                "\"",
                "_",
                "d"
            ]
        },
        {
            "before": [
                "<Tab>"
            ],
            "after": [
                "<S>",
                ">",
                "g",
                "v"
            ]
        },
        {
            "before": [
                "<S-Tab>",
            ],
            "after": [
                "<S>",
                "<",
                "g",
                "v"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "d"
            ],
            "after": [
                "\"",
                "_",
                "d"
            ]
        },
        {
            "before": [
                "D"
            ],
            "after": [
                "\"",
                "_",
                "D"
            ]
        },
        {
            "before": [
                "d",
                "d"
            ],
            "after": [
                "\"",
                "_",
                "d",
                "d"
            ]
        },
        {
            "before": [
                "<BS>"
            ],
            "after": [
                "\"",
                "_",
                "d"
            ]
        },
        {
            "before": [
                "<leader>",
                "d"
            ],
            "after": [
                "d",
                "d"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "K"
            ],
            "commands": [
                {
                    "command": "editor.action.showDefinitionPreviewHover"
                }
            ]
        },
        {
            "before": [
                "s"
            ],
            "after": [
                "leader",
                "leader",
                "s"
            ]
        },
        {
            "before": [
                "<Tab>"
            ],
            "after": [
                "<S>",
                ">",
                ">"
            ]
        },
        {
            "before": [
                "<S-Tab>",
            ],
            "after": [
                "<S>",
                "<",
                "<"
            ]
        }
    ],
    "vim.leader": "<space>",
